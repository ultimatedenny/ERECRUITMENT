    function SAVE_SIBLINGS() {
        var URL = "@Url.Action("WEB_CANDIDATE_DETAILS_SAVE_SIBLINGS", "INTERVIEWBOARD")";
        var BATCH_EMP = "@ViewBag.BATCH_EMP";
        var SIBLINGS = new Array();
        for (var i = 0; i < TOTAL_SIBLINGS; i++) {
            var _ID = "";
            var _NAME = "";
            var _AGE = "";
            var _JOB = "";
            if (i == 0) {
                _ID = document.getElementById("EMP_SIBLINGS_ID0").value;
                _NAME = document.getElementById("EMP_SIBLINGS_NAME0").value;
                _AGE = document.getElementById("EMP_SIBLINGS_AGE0").value;
                _JOB = document.getElementById("EMP_SIBLINGS_JOB0").value;
            }
            else {
                _ID = document.getElementById("EMP_SIBLINGS_ID" + i + "").value;
                _NAME = document.getElementById("EMP_SIBLINGS_NAME" + i + "").value;
                _AGE = document.getElementById("EMP_SIBLINGS_AGE" + i + "").value;
                _JOB = document.getElementById("EMP_SIBLINGS_JOB" + i + "").value;
            }
            SIBLINGS[i] = { ID: _ID, NAME: _NAME, AGE: _AGE, JOB: _JOB };
        }
        SIBLINGS = JSON.stringify({ 'MODEL': SIBLINGS });
        $.ajax({
            contentType: 'application/json; charset=utf-8',
            dataType: 'json',
            type: 'POST',
            url: URL,
