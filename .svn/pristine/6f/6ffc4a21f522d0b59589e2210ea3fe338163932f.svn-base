        function CAPTURE(i) {
            $('#exampleModalCenter').modal('show');
            $("#exampleModalCenterTitle").text(i);
            Webcam.set({
                width: 180,
                height: 320,
                image_format: 'jpeg',
                jpeg_quality: 100,
                constraints: {
                    width: 180, // { exact: 320 },
                    height: 320, // { exact: 180 },
                    facingMode: 'user',
                    frameRate: 30
                }
            });
            Webcam.attach('#my_camera');
        }
        function SAVE_CAPTURE() {
            var i = $("#exampleModalCenterTitle").text();
            var RESULT = document.getElementById("RESULT_[" + i + "]");
            Webcam.snap(function (data_uri) {
                RESULT.innerHTML = '<img src="' + data_uri + '"/>';
            });
