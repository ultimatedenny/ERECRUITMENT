    <script src="~/Crox/plugins/highlight/highlight.pack.js"></script>
    <script src="~/Crox/assets/js/custom.js"></script>
    <script src="~/Crox/assets/js/scrollspyNav.js"></script>
    <script src="~/Crox/plugins/bootstrap-select/bootstrap-select.min.js"></script>
    <script src="~/Crox/plugins/notification/snackbar/snackbar.min.js"></script>
    <script src="~/Crox/assets/js/components/notification/custom-snackbar.js"></script>
    <script src="~/Crox/plugins/table/datatable/datatables.js"></script>
    <script src="~/Crox/plugins/flatpickr/flatpickr.js"></script>
    <script src="~/Crox/plugins/noUiSlider/nouislider.min.js"></script>
    <script src="~/Crox/plugins/flatpickr/custom-flatpickr.js"></script>
    <script src="~/Crox/plugins/noUiSlider/custom-nouiSlider.js"></script>
    <script src="~/Crox/plugins/bootstrap-range-Slider/bootstrap-rangeSlider.js"></script>
    <script type="text/javascript" src="~/Crox/CustomPlugins/DataTablesCheckBox/js/dataTables.checkboxes.min.js"></script>
    <script src="~/Crox/plugins/select2/select2.min.js"></script>
    <script src="~/Crox/plugins/blockui/jquery.blockUI.min.js"></script>
    <script src="~/Crox/plugins/blockui/custom-blockui.js"></script>
    <script src="~/Crox/plugins/bootstrap-touchspin/jquery.bootstrap-touchspin.min.js"></script>
    <script src="~/Crox/plugins/bootstrap-touchspin/custom-bootstrap-touchspin.js"></script>
    <script src="~/Crox/plugins/input-mask/jquery.inputmask.bundle.min.js"></script>
    <script src="~/Crox/plugins/input-mask/input-mask.js"></script>

    @{
        if (TempData["ERROR"] != null)
        {
            <script type="text/javascript">
                Snackbar.show({
                    text: '@TempData["ERROR"]',
                    pos: 'bottom-right'
                });
            </script>
        }
        if (TempData["SUCCESS"] != null)
        {
            <script type="text/javascript">
                Snackbar.show({
                    text: '@TempData["SUCCESS"]',
                    pos: 'bottom-right'
                });
            </script>
        }
    }
</body>
</html>