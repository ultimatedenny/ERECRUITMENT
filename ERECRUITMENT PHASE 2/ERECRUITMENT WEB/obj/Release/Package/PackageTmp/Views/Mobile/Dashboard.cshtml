@{
    ViewBag.Title = "Profile";
    Layout = "~/Views/Shared/_Layout_Mobile.cshtml";
}
<div class="layout-px-spacing">
    <div class="row layout-top-spacing">
        <div class="col-xl-12 col-lg-12 col-md-12 layout-spacing">
