    function UPLOAD_BCA() {
        $.blockUI({
            message: "UPLOADING BCA..",
            fadeIn: 500,
            overlayCSS: {
                backgroundColor: '#1b2024',
                opacity: 0.8,
                zIndex: 1200,
                cursor: 'wait'
            },
            css: {
                border: 0,
                color: '#fff',
                zIndex: 1201,
                padding: 0,
                backgroundColor: 'transparent'
            }
        });
        var fileData = new FormData();
        var fileUpload = $("#NO_BCA_FILE").get(0);
        var files = fileUpload.files;
        for (var i = 0; i < files.length; i++) {
            fileData.append('File', files[i]);
        }
        var URL = "@Url.Action("UPLOAD_BCA", "MOBILE")";
        $.ajax({
            url: URL,
            method: 'POST',
            contentType: false,
            processData: false,
            data: fileData,
            success: function (data) {
                if (data.Result == true) {
                    UPLOAD_NPWP();
                }
                else {
                    $.unblockUI();
                    Snackbar.show({ text: 'UPLAD_BCA() ERROR: ' + data.Message + '', pos: 'bottom-right' });
                }
            },
            error: {
                function(request, status, error) {
                    $.unblockUI();
                    Snackbar.show({ text: 'UPLAD_BCA() ERROR: ' + request.responseText + '', pos: 'bottom-right' });
                }
            }
        });
    }

