    function UPLOAD_DATA() {
        $.blockUI({
            message: "SAVING DATA..",
            fadeIn: 500,
            overlayCSS: {
                backgroundColor: '#1b2024',
                opacity: 0.8,
                zIndex: 1200,
                cursor: 'wait'
            },
            css: {
                border: 0,
                color: '#fff',
                zIndex: 1201,
                padding: 0,
                backgroundColor: 'transparent'
            }
        });
        var URL = "@Url.Action("MOBILE_SAVE_DOCUMENT", "MOBILE")";
        var UNIQUEID = "@ViewBag.UNIQUEID";
        var ACCOUNT_BCA = document.getElementById('NO_BCA_LABEL').value;
        var ACCOUNT_NPWP = document.getElementById('NO_NPWP_LABEL').value;
        var ACCOUNT_EFIN = document.getElementById('NO_EFIN_LABEL').value;
        var ACCOUNT_BPJS_KES = document.getElementById('NO_BPJS_KS_LABEL').value;
        var ACCOUNT_BPJS_TKJ = document.getElementById('NO_BPJS_TKJ_LABEL').value;

        if (ACCOUNT_EFIN == null) {
            ACCOUNT_EFIN = "";
        }
        if (ACCOUNT_BPJS_KES == null) {
            ACCOUNT_BPJS_KES = "";
        }
        if (ACCOUNT_BPJS_TKJ == null) {
            ACCOUNT_BPJS_TKJ = "";
        }

        var MODEL = {
            BATCH_EMP: UNIQUEID,
            ACCOUNT_BCA: ACCOUNT_BCA,
            ACCOUNT_NPWP: ACCOUNT_NPWP,
            ACCOUNT_EFIN: ACCOUNT_EFIN,
            ACCOUNT_BPJS_KES: ACCOUNT_BPJS_KES,
            ACCOUNT_BPJS_TKJ: ACCOUNT_BPJS_TKJ
        };
        $.ajax({
            type: "POST",
            url: URL,
            data: '{MODEL: ' + JSON.stringify(MODEL) + '}',
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            success: function (data) {
                if (data.Result == true) {
                    $.unblockUI();
                    setTimeout(function () {
                        Snackbar.show({ text: 'DOCUMENT DATA UPDATED, PAGE WILL BE REDIRECT TO DASHBOARD.', pos: 'bottom-right' });
                        setTimeout(function () {
                            window.location.href = "@Url.Action("DASHBOARD", "MOBILE")?id=" + UNIQUEID;
                        }, 500);
                    }, 1000);
                }
                else {
                    $.unblockUI();
                    Snackbar.show({ text: 'UPLOAD_DATA() ERROR: ' + data.Message + '', pos: 'bottom-right' });
                }
            },
