    <link rel="stylesheet" type="text/css" href="~/Crox/plugins/table/datatable/dt-global_style.css">
    <link href="~/Crox/plugins/flatpickr/flatpickr.css" rel="stylesheet" type="text/css">
    <link href="~/Crox/plugins/noUiSlider/nouislider.min.css" rel="stylesheet" type="text/css">
